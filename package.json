    "lodash": "^4.17.4",
    "mongodb": "^2.2.33 ",
    "mongoose": "^5.0.0-rc1",
    "mongoose-mongodb-errors": "0.0.2",
    "morgan": "~1.9.0",
    "passport": "^0.4.0",
